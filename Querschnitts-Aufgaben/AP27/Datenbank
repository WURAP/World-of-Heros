
CREATE TABLE Benutzertyp (
Email VARCHAR(30) NOT NULL, 
Vorname VARCHAR(30) NOT NULL, 
Nachname VARCHAR(30), 
Adresse VARCHAR(60),
Typ VARCHAR(20), 
PRIMARY KEY (Email)
);

CREATE TABLE Profil (
Profil_ID INTEGER NOT NULL,
Profilname VARCHAR(30),
Erstellungsdatum VARCHAR(10) NOT NULL,
Letzter_Login VARCHAR(10) NOT NULL,
Aktivierung VARCHAR(4) NOT NULL,
Reset_Token VARCHAR(4) NOT NULL,
Secret_Link VARCHAR(60) NOT NULL,
Sicherheitsfrage VARCHAR(30) NOT NULL,
Passwort VARCHAR(15) NOT NULL,
Email VARCHAR(60) NOT NULL, 
PRIMARY KEY (Profilname)
);

CREATE TABLE FIGUR (
Figur_ID INTEGER NOT NULL,
Levelnummer INTEGER NOT NULL,
PRIMARY KEY (Figur_ID)
);

CREATE TABLE FIGURINSTANZ (
Figurinstanz_ID INTEGER NOT NULL, 
Geschlecht VARCHAR(30) NOT NULL,
Kopf VARCHAR(60) NOT NULL,
Rumpf VARCHAR(60) NOT NULL,
Beine VARCHAR(60) NOT NULL,
Levelnummer INTEGER NOT NULL,
Accessoire VARCHAR(60) NOT NULL,
Figur_ID INTEGER NOT NULL,
PRIMARY KEY (Figurinstanz_ID)
);

CREATE TABLE SPIELTYP (
Spieltyp_ID INTEGER NOT NULL,
Spieltyp_Name VARCHAR(60) NOT NULL,
PRIMARY KEY (Spieltyp_ID)
);

CREATE TABLE Spiel (
Spiel_ID INTEGER NOT NULL,
Spieltyp_ID INTEGER NOT NULL,
PRIMARY KEY (Spiel_ID)
);

CREATE TABLE Spielstufe (
Level_ID VARCHAR(9) NOT NULL,
Levelnummer VARCHAR(20) NOT NULL,
Spiel_ID INTEGER NOT NULL,
PRIMARY KEY (Level_ID)
);

CREATE TABLE Therapiegruppe (
Therapiegruppenname VARCHAR(60) NOT NULL,
Patient_Name VARCHAR(60) NOT NULL,
Therapeut_Name VARCHAR(60) NOT NULL,
PRIMARY KEY (Therapiegruppenname)
);

CREATE TABLE Therapietyp (
Therapietyp_ID INTEGER NOT NULL, 
Therapietypbezeichnung VARCHAR(40) NOT NULL, 
Therapiegruppenname VARCHAR(60) NOT NULL,
PRIMARY KEY (Therapietyp_ID) 
);

CREATE TABLE Therapie (
Therapie_ID INTEGER NOT NULL,
Therapietyp_ID INTEGER NOT NULL,
PRIMARY KEY (Therapie_ID)
);

CREATE TABLE Sitzung (
Sitzungsteilnehmer VARCHAR(40) NOT NULL,
Sitzungsdatum VARCHAR(40) NOT NULL, 
Therapie_ID INTEGER NOT NULL,
PRIMARY KEY (Sitzungsteilnehmer,Sitzungsdatum)
);

CREATE TABLE Score ( 
Wert INTEGER NOT NULL,
Sitzungsteilnehmer VARCHAR(40) NOT NULL,
Sitzungsdatum VARCHAR(40) NOT NULL, 
Level_ID VARCHAR(9) NOT NULL,
Figurinstanz_ID INT NOT NULL,
FOREIGN KEY (Sitzungsteilnehmer,Sitzungsdatum) REFERENCES Sitzung(Sitzungsteilnehmer,Sitzungsdatum),
FOREIGN KEY (Level_ID) REFERENCES Spielstufe(Level_ID),
FOREIGN KEY (Figurinstanz_ID) REFERENCES Figurinstanz(Figurinstanz_ID),
PRIMARY KEY (Sitzungsteilnehmer,Sitzungsdatum,Level_ID,Figurinstanz_ID)
);

ALTER TABLE Profil 
ADD FOREIGN KEY (Email) REFERENCES Benutzertyp(Email);

ALTER TABLE Figurinstanz 
ADD FOREIGN KEY (Figur_ID) REFERENCES Figur(Figur_ID); 

ALTER TABLE Therapietyp
ADD FOREIGN KEY (Therapiegruppenname) REFERENCES Therapiegruppe(Therapiegruppenname);

ALTER TABLE Therapie
ADD FOREIGN KEY (Therapietyp_ID) REFERENCES Therapietyp(Therapietyp_ID);

ALTER TABLE Sitzung
ADD FOREIGN KEY (Therapie_ID) REFERENCES Therapie(Therapie_ID);

ALTER TABLE Spiel
ADD FOREIGN KEY (Spieltyp_ID) REFERENCES Spieltyp(Spieltyp_ID);

ALTER TABLE Spielstufe
ADD FOREIGN KEY (Spiel_ID) REFERENCES Spiel(Spiel_ID);

